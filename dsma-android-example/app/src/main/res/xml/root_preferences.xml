<androidx.preference.PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory app:title="@string/sdkms_header">

        <EditTextPreference
            app:key="fortanix_endpoint"
            app:title="@string/fortanix_endpoint"
            app:useSimpleSummaryProvider="true" />

        <EditTextPreference
            app:key="apikey"
            app:title="@string/apikey"
            app:useSimpleSummaryProvider="true" />

        <EditTextPreference
            app:key="key_uuid"
            app:title="@string/key_uuid"
            app:useSimpleSummaryProvider="true" />

        <EditTextPreference
            app:key="cache_duration"
            app:title="@string/cache_duration"
            app:useSimpleSummaryProvider="true" />

    </PreferenceCategory>

</androidx.preference.PreferenceScreen>
